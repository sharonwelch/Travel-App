<%= link_to 'Sign Out', destroy_user_session_path, method: 'DELETE' %>
<body>
  <div id="fullpage">
    <div class="section intro">
      <p class="greeting">
      <%= @greeting %>
      </p>
      <br>
      <%= @mission_statement %>
    </div>
    <div class="section stories">
      <%= @browse_locals %>
      <br>
      <a type= "button" class="btn btn-primary-outline" href="/instructors" role="button">
          Discover Activities
      </a>
    </div>

    <div class="section apply">
      Apply as a local
      <br>
      <a href="/instructor_apps/new">
        Application
      </a>
      <br>
      Sign up as a traveller
      <br>
      <a href="/users/sign_up">
        Sign Up
      </a>
    </div>
  </div>
</body>

