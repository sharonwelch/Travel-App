<div id="fullpage">
  <div class="section intro">
    <p class="greeting">
    <%= @greeting %>
    </p>
    <br>
    <%= @mission_statement %>
  </div>
  <div class="section stories">
    <%= @browse_locals %>
    <br>
    <a class="btn btn-primary" href="/instructors" role="button">
        Discover Activities
    </a>
  </div>

  <div class="section apply">
    <a href="/instructor_apps/new">
      Application
    </a>
    <a href="/users/sign_up">
      Sign Up
    </a>
    <%= link_to 'Sign Out', destroy_user_session_path, method: 'DELETE' %>
  </div>

</div>

